{
  "domains": [
    "yourdomain.com","*.mydomain.com"
  ],
  "white": {
    "action": "error",
    "folder": {
      "names": [
        "white"
      ]
    },
    "redirect": {
      "type": "302",
      "urls": [
        "https://ya.ru",
        "https://google.com"
      ]
    },
    "curl": {
      "urls": [
        "https://www.9111.ru/"
      ]
    },
    "error": {
      "codes": [
        "200"
      ]
    },
    "domainfilter": {
      "use": false,
      "domains": [
        {
          "name": "127.0.0.1",
          "action": "folder:white"
        },
        {
          "name": "localhost",
          "action": "folder:white2"
        }
      ]
    },
    "jschecks": {
      "enabled": false,
      "events": [
        "audiocontext",
        "timezone"
      ],
      "timeout": "40000",
      "obfuscate": true,
      "timezone": true,
      "tzstart": "-3",
      "tzend": "4"
    }
  },
